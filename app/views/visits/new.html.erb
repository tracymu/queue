<h1>If you've been to <%= @restaurant.name %></h1>

<p>Help your friends out by rating and/or reviewing </p>

<%= form_for [@restaurant, @visit] do |f| %>
       <%= f.label :rating %>
      <%= f.text_field :rating %>
  
      <%= f.label :review %>
      <%= f.text_area :review %>
		</br>
    <%= f.submit %>

<% end %>
